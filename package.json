{
    "name": "jobsite-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint"
    },
    "dependencies": {
        "@clerk/nextjs": "^6.19.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.2",
        "@sentry/nextjs": "^9.15.0",
        "chart.js": "^4.4.9",
        "daisyui": "^5.0.35",
        "date-fns": "^4.1.0",
        "graphql": "^16.11.0",
        "graphql-request": "^7.1.2",
        "next": "15.3.1",
        "next-themes": "^0.4.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-toastify": "^11.0.5",
        "resend": "^4.4.1",
        "semantic-release": "^24.2.3",
        "tailwind-merge": "^3.2.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "eslint": "^9",
        "eslint-config-next": "15.3.1",
        "raw-loader": "^4.0.2",
        "tailwindcss": "^4.1.5"
    },
    "configurations": [
        {
            "name": "Next.js: debug server-side",
            "type": "node-terminal",
            "request": "launch",
            "command": "npm run dev",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "/turbopack/[project]/*": "${webRoot}/*"
            }
        },
        {
            "name": "Next.js: debug client-side",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "/turbopack/[project]/*": "${webRoot}/*"
            }
        }
    ]
}
